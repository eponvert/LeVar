POST    /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/new_experiment                                         controllers.api.ExperimentController.create(org: String, setId: String)
GET     /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/$id<[-\w\.]+>                                          controllers.api.ExperimentController.details(org: String, setId: String, id: String)
POST    /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/$id<[-\w\.]+>/update                                   controllers.api.ExperimentController.update(org: String, setId: String, id: String)
