POST    /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/data/$id<[-\w\.]+>/label                               controllers.api.LabelController.addForDatum(org: String, setId: String, id: String)
DELETE  /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/data/$id<[-\w\.]+>/label/$label<[\w]+>                 controllers.api.LabelController.removeFromDatum(org: String, setId: String, id: String, label: String)
GET     /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/data/$id<[-\w\.]+>/comments                            controllers.api.CommentController.searchByDatum(org: String, setId: String, id: String)
POST    /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/data/$id<[-\w\.]+>/comment                             controllers.api.CommentController.addForDatum(org: String, setId: String, id: String)
GET     /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/data/$id<[-\w\.]+>/comment/$cid<[0-9a-f]+>             controllers.api.CommentController.viewForDatum(org: String, setId: String, id: String, cid: String)
