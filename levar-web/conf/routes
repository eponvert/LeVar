DELETE  /api/$org<[-\w\.]+>/$setId<[-\w\.]+>/$expId<[-\w\.]+>/$id<[-\w\.]+>/label/$label<[\w]+>     controllers.api.LabelController.removeFromPrediction(org: String, setId: String, expId: String, id: String, label: String)
